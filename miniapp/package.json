{
  "name": "miniapp",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "./deploy.sh",
    "build:version": "echo \"export const BUILD_INFO = { buildTime: '$(date -u +\"%Y-%m-%dT%H:%M:%SZ\")', gitHash: '$(git rev-parse --short HEAD 2>/dev/null || echo \"unknown\")', version: '$(node -p \"require('./package.json').version\")' };\" > src/build-info.ts && npm run build"
  },
  "dependencies": {
    "@twa-dev/sdk": "^8.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intersection-observer": "^9.16.0",
    "react-parallax-tilt": "^1.7.309",
    "react-swipeable": "^7.0.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
